{
  "name": "derhub/integration",
  "description": "",
  "authors": [
    {
      "name": "Johnder Baul",
      "email": "imjohnderbaul@gmail.com"
    }
  ],
  "require": {
    "league/tactician": "^1.1",
    "jms/serializer": "^3.12"
  },
  "require-dev": {
    "derhub/shared": "dev-master",
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Derhub\\Integration\\": "src/",
      "Derhub\\Laravel\\Integration\\": "integration/laravel/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Integration\\": "tests",
      "Tests\\Laravel\\Integration\\": "integration/laravel/tests"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
