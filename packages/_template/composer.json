{
  "name": "derhub/template",
  "description": "",
  "type": "template",
  "authors": [
    {
      "name": "Johnder Baul",
      "email": "imjohnderbaul@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Derhub\\Template\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Template\\": "tests"
    }
  },
  "scripts": {
    "phinx": "@php vendor/bin/phinx",
    "psalm": "../../vendor/bin/psalm ."
  },
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "derhub/integration": "dev-master",
    "derhub/shared": "dev-master",
    "derhub/shared-database": "dev-master",
    "phpunit/phpunit": "^9.5"
  }
}
